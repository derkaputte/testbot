<?xml version="1.0"?>
<robot name="visual">


<!-- MATERIALS -->
<material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>


<!-- BASE -->
  <link name="base_link">
    <visual>
      <origin rpy="0 0 -1.57075" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://testbot/meshes/Base.dae"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <inertial>
      <origin rpy="0 0 -1.57075" xyz="0 0 0"/>
      <mass value="1.5" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <collision>
      <origin rpy="0 0 -1.57075" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://testbot/meshes/Base.dae"/>
      </geometry>
    </collision>
  </link>

<!-- DOME -->
    
  <link name="dome">
    <visual>
      <geometry>
        <mesh filename="package://testbot/meshes/dome.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.5" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://testbot/meshes/dome.dae"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_to_dome" type="fixed">
    <parent link="base_link"/>
    <child link="dome"/>
    <origin xyz="0 0 0.005"/>
  </joint> 
    
<!-- KINECT -->
    
  <link name="kinect">
    <visual>
      <geometry>
        <mesh filename="package://testbot/meshes/Kinect.dae"/>
      </geometry>
      <origin rpy="0 0 -1.57075" xyz="0 0 0"/>
    </visual>
    <inertial>
      <origin rpy="0 0 -1.57075" xyz="0 0 0"/>
      <mass value="0.3" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <collision>
      <origin rpy="0 0 -1.57075" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://testbot/meshes/Kinect.dae"/>
      </geometry>
    </collision>
  </link>

  <joint name="dome_to_kinect" type="fixed">
    <parent link="dome"/>
    <child link="kinect"/>
    <origin xyz="0 0 0.25"/>
  </joint>     
    
<!-- WHEELS -->

  <link name="wheel_r">
    <visual>
      <geometry>
        <mesh filename="package://testbot/meshes/Rad_gelb.dae"/>
      </geometry>
      <origin rpy="0 0 1.57075" xyz="0 0 0"/>
    </visual>
    <inertial>
      <origin rpy="0 0 1.57075" xyz="0 0 0"/>
      <mass value="0.2" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <collision>
      <origin rpy="0 0 1.57075" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://testbot/meshes/Rad_gelb.dae"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_to_wheel_r" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_r"/>
		<axis xyz="0 1 0"/>
    <origin xyz="0 -0.19 0.035"/>
	        <hardwareInterface>VelocityJointInterface</hardwareInterface>
  </joint>  

    <gazebo reference="wheel_r">
      <mu1 value="200.0"/>
      <mu2 value="100.0"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>

  <transmission name="wheel_r_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="wheel_r_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>





  <link name="wheel_l">
    <visual>
      <geometry>
        <mesh filename="package://testbot/meshes/Rad_gelb.dae"/>
      </geometry>
      <origin rpy="0 0 1.57075" xyz="0 0 0"/>
    </visual>
    <inertial>
      <origin rpy="0 0 1.57075" xyz="0 0 0"/>
      <mass value="0.2" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <collision>
      <origin rpy="0 0 1.57075" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://testbot/meshes/Rad_gelb.dae"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_to_wheel_l" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_l"/>
		<axis xyz="0 1 0"/>
    <origin xyz="0 0.19 0.035"/>
	        <hardwareInterface>VelocityJointInterface</hardwareInterface>
  </joint>  

    <gazebo reference="wheel_l">
      <mu1 value="200.0"/>
      <mu2 value="100.0"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>

  <transmission name="wheel_l_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="wheel_l_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <link name="einhausung">
    <visual>
      <geometry>
        <mesh filename="package://testbot/meshes/Einhausung.dae"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1.0" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://testbot/meshes/Einhausung.dae"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_to_einhausung" type="fixed">
    <parent link="base_link"/>
    <child link="einhausung"/>
    <origin xyz="-0.145 0 0.005"/>
  </joint>

  <link name="stuetz">
    <visual>
      <geometry>
        <mesh filename="package://testbot/meshes/Stuetzrad.dae"/>
      </geometry>
      <origin rpy="0 0 1.5707963268" xyz="0 0 0"/>
    </visual>
    <inertial>
      <origin rpy="0 0 1.5707963268" xyz="0 0 0"/>
      <mass value="0.2" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <collision>
      <origin rpy="0 0 1.5707963268" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://testbot/meshes/Stuetzrad.dae"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_to_stuetz" type="fixed">
    <parent link="base_link"/>
    <child link="stuetz"/>
    <origin xyz="-0.145 0 0.049"/>
  </joint>

  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/</robotNamespace>
    </plugin>
  </gazebo>

</robot>
